            "example": "a123456789"
          },
          "email": {
            "type": "string",
            "example": "pepe@gmail.com"
          },
          "age": {
            "type": "number",
            "example": 33
          },
          "city": {
            "type": "string",
            "example": "SEG"
          }
        },
        "required": [
          "name",
          "surname",
          "role",
          "password",
          "email",
          "age",
          "city"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "name": {
            "type": "string",
            "example": "Pepe"
          },
          "surname": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "example": "USER",
            "enum": [
              "USER",
              "ADMIN"
            ]
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "age": {
            "type": "number"
          },
          "city": {
            "type": "string"
          },
          "fullname": {
            "type": "string",
            "example": "Pepe Gonzalez"
          }
        },
        "required": [
          "id",
          "name",
          "surname",
          "role",
          "createdAt",
          "email",
          "age",
          "city",
          "fullname"
        ]
      },
      "BadRequest": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "example": "/*"
          },
          "timestamp": {
            "format": "date-time",
            "type": "string",
            "example": "2021-04-30T15:12:23.562Z"
          },
          "message": {
            "example": [
              "* must be uppercase",
